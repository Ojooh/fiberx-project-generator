{
    "name": "<%= full_project_name %>",
    "version": "<%= version %>",
    "description": "<%= description %>",
    "private": true,
    "scripts": {
        "dev": "vite --config ./vite.config.mjs",
        "devcss": "npx tailwindcss -i ./public/assets/css/input.css -o ./public/assets/css/output.css --watch",
        "build": "vite build --config ./vite.config.mjs",
        "preview": "vite preview --config ./vite.config.mjs",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "format": "prettier --write src/"
    },
    "author": "<%= author %>",
    "license": "<%= license %>",
    "repository": {
		"type": "git",
		"url": "<%= repository %>"
    },
    "keywords": <%- JSON.stringify(keywords) %>,
    "dependencies": {
        "@tailwindcss/postcss": "^4.1.8",
        "@tailwindcss/vite": "^4.1.8",
        "axios": "^1.6.7",
        "flowbite": "^2.3.0",
        "flowbite-datepicker": "^1.2.6",
        "js-cookie": "^3.0.5",
        "moment": "^2.30.1",
        "postcss": "^8.5.4",
        "socket.io-client": "^4.8.1",
        "vue": "^3.5.13",
        "vue-router": "^4.3.0",
        "vue3-tel-input": "^1.0.4",
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.2.3",
        "tailwindcss": "^4.1.8",
        "vite": "^6.3.5"
    }
}
  
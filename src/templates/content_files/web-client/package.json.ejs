{
    "name": "<%= full_project_name %>",
    "version": "<%= version %>",
    "description": "<%= description %>",
    "private": true,
    "scripts": {
        "dev": "vite --config ./vite.config.js",
        "devcss": "npx tailwindcss -i ./public/assets/css/input.css -o ./public/assets/css/output.css --watch",
        "build": "vite build --config ./vite.config.js",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "format": "prettier --write src/"
    },
    "author": "<%= author %>",
    "license": "<%= license %>",
    "repository": {
		"type": "git",
		"url": "<%= repository %>"
    },
    "keywords": <%- JSON.stringify(keywords) %>,
    "dependencies": {
        "axios": "^1.6.7",
        "flowbite": "^2.3.0",
        "flowbite-datepicker": "^1.2.6",
        "js-cookie": "^3.0.5",
        "moment": "^2.30.1",
        "vue": "^3.4.21",
        "vue-router": "^4.3.0",
        "vue3-tel-input": "^1.0.4",
    }
}
  